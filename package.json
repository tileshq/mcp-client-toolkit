{
  "name": "mcp-client-toolkit",
  "version": "1.0.0",
  "description": "TypeScript client library and utilities for Model Context Protocol (MCP) servers",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "postinstall": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "client",
    "oauth",
    "sdk",
    "library",
    "toolkit"
  ],
  "author": "Aswin C",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "tsconfig.json",
    "README.md"
  ]
}